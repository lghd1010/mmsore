<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="DAO.BranchDAO">
    	
    	<select id="memberbranchlist" resultType="DTO.BranchDTO">
    		select * from branch
    		where b_mensa_no = 1
    		and b_active = 0
    	</select>
    
    </mapper>